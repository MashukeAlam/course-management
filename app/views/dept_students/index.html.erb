<p id="notice"><%= notice %></p>

<h1>Dept Students</h1>

<table>
  <thead>
    <tr>
      <th>Department</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dept_students.each do |dept_student| %>
      <tr>
        <td><%= dept_student.department_id %></td>
        <td><%= dept_student.user_id %></td>
        <td><%= link_to 'Show', dept_student %></td>
        <td><%= link_to 'Edit', edit_dept_student_path(dept_student) %></td>
        <td><%= link_to 'Destroy', dept_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dept Student', new_dept_student_path %>
