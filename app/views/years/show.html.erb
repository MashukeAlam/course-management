<p id="notice"><%= notice %></p>

<p>
  <strong>Department:</strong>
  <%= @year.department_id %>
</p>

<table>
  <thead>
  <tr>
    <th>Semesters</th>
  </tr>
  </thead>

  <tbody>
  <% @year.semesters.each do |sem| %>
