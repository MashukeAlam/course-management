<p id="notice"><%= notice %></p>

<h1>Semester Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Semester</th>
      <th>Subject</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @semester_subjects.each do |semester_subject| %>
      <tr>
        <td><%= semester_subject.semester_id %></td>
        <td><%= semester_subject.subject_id %></td>
        <td><%= link_to 'Show', semester_subject %></td>
        <td><%= link_to 'Edit', edit_semester_subject_path(semester_subject) %></td>
        <td><%= link_to 'Destroy', semester_subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Semester Subject', new_semester_subject_path %>
